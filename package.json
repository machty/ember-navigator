  "engines": {
    "node": ">= 12",
    "yarn": "^1.22.18"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "homepage": "https://github.com/machty/ember-navigator",
  "resolutions": {
    "@ember/test-helpers": "^2.4.0",
    "ember-cli-babel": "^7.26.6",
    "@babel/parser": "7.16.4",
    "minimist": "^1.2.6"
  }
