{
  "name": "ember-navigator",
  "version": "1.0.0-alpha.5",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Alex Matchneer",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "release-it": {
    "npm": {
      "skipChecks": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": "https://github.com/machty/ember-navigator",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "release": "release-it",
    "start": "NODE_OPTIONS=--openssl-legacy-provider ember server",
    "test": "NODE_OPTIONS=--openssl-legacy-provider ember test"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.4",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^5.0.0",
    "process-nextick-args": "^1.0.7",
    "util-deprecate": "^1.0.2"
